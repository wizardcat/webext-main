{
  "name": "webext",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "manifest_version": 3,
  "options_page": "options.html",
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval' http://localhost;object-src 'self';"
  },
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "96": "icon/96.png",
    "128": "icon/128.png"
  },
  "permissions": ["offscreen", "scripting", "storage", "tabs"]
}
